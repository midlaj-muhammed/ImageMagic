# ImageMagic Server Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3001
NODE_ENV=development

# Hugging Face Configuration (Optional)
# Get a free token from https://huggingface.co/settings/tokens
# This is optional - the app works without it using free Hugging Face Spaces
HF_TOKEN=your_hugging_face_token_here

# CORS Configuration
CORS_ORIGIN=http://localhost:8082

# Hugging Face Spaces Configuration
# Using free Hexii/Neural-Style-Transfer space
HF_SPACE_URL=https://hexii-neural-style-transfer.hf.space

# Logging
LOG_LEVEL=info
